#%RAML 1.0

# Objective: This RAML validates a property marked as not required using two trailing question marks in the name, and no explicit required key
# Expected result: display name title? , no missing required field
# Validated against a valid example

# tags: object, properties, trailing-question-mark

title: properties with trailing question marks
types:
  Person:
    type: object
    properties:
      firstname: string
      lastname:  string
      title??:    string
    example:
      firstname: eleo
      lastname:  ortega
